{
  "name": "automatopagamento",
  "version": "1.0.0",
  "description": "Sistema para extrair dados de pagamentos PIX de imagens e PDFs",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "pix",
    "ocr",
    "pdf",
    "pagamento",
    "automatizacao"
  ],
  "author": "Sidnei",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "pdf-parse": "^2.4.5",
    "pdf2pic": "^3.2.0",
    "tesseract.js": "^6.0.1"
  }
}
